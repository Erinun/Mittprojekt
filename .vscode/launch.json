    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Run ansi-html",
            "runtimeExecutable": "ansi-html",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "https://processkartapaket2025.netlify.app/",
            "webRoot": "${workspaceFolder}"
        }
    ]
}